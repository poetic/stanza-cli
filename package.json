{
  "name": "stanza",
  "version": "2.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "stanza": "bin/stanza"
  },
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src -d lib --watch",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "docs": "jsdoc src -c jsdoc.conf.json",
    "gh-pages": "gh-pages -d docs",
    "start": "npm run watch"
  },
  "description": "Stanza is a highly extensible cli that accelerates development workflows.",
  "private": true,
  "author": "Poetic Systems",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "gh-pages": "^0.11.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.0"
  },
  "dependencies": {
    "app-root-path": "^1.3.0",
    "commander": "^2.9.0",
    "find-up": "^1.1.2",
    "junk": "^2.0.0",
    "lodash": "^4.15.0",
    "pkg-conf": "^1.1.3",
    "resolve": "^1.1.7",
    "yeoman-environment": "^1.6.3"
  }
}
